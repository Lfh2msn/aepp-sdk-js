    "babel-loader": "^8.2.3",
    "babel-plugin-add-import-extension": "^1.6.0",
    "babel-plugin-transform-default-named-imports": "^1.2.2",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.8.3",
    "eslint": "^7.32.0",
