    "@stamp/required": "^1.0.1",
    "aes-js": "^3.1.2",
    "bignumber.js": "^9.0.2",
    "bip32-path": "^0.4.2",
    "blakejs": "^1.1.1",
    "bs58check": "^2.1.2",
    "buffer": "^6.0.3",
    "cross-fetch": "^3.1.5",
    "events": "^3.3.0",
    "process": "^0.11.10",
    "rlp": "^2.2.7",
    "sha.js": "^2.4.11",
    "stream-browserify": "^3.0.0",
    "swagger-client": "^3.18.2",
    "tweetnacl": "^1.0.3",
    "tweetnacl-auth": "^1.0.1",
    "websocket": "^1.0.34"
